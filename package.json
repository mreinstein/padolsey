{
  "name": "padolsey",
  "version": "1.0.0",
  "description": "generate padolsey patterns",
  "main": "dist/padolsey.cjs.js",
  "module": "dist/padolsey.esm.js",
  "scripts": {
    "build": "npm-run-all -p build:*",
    "build:cjs": "rollup index.js --file dist/padolsey.cjs.js --format cjs",
    "build:esm": "rollup index.js --file dist/padolsey.esm.js --format esm",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mreinstein/padolsey.git"
  },
  "keywords": [
    "padolsey",
    "pattern",
    "procedural texture",
    "texture"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mreinstein/padolsey/issues"
  },
  "homepage": "https://github.com/mreinstein/padolsey#readme",
  "devDependencies": {
    "npm-run-all": "^4.1.3",
    "rollup": "^0.67.1"
  }
}
